  <form onSubmit="return checkAnswer();">
  <input id="replicaSetID" type="text" class="box" autofocus />
  <input type="submit" class="submit" value="SUBMIT" />
  </form>

  <script>
  function checkAnswer(){
      var response = document.getElementById('replicaSetID').value;
      location = response;
      return false;
  }
  </script>
